   <Object type="0x11c8" id="Staff of the Pumpkin King">
      <Class>Equipment</Class>
      <Item/>
      <Texture>
         <File>newObjects</File>
         <Index>0x50</Index>
      </Texture>
      <SlotType>17</SlotType>
      <Description>
      Carved pumpkins imbued with magic light up with the weapon's wearer, imitating the pumpkin king.
      </Description>
      <RateOfFire>1</RateOfFire>
      <Sound>weapon/wand_of_death</Sound>
      <Projectile>
         <ObjectId>Orange Missile</ObjectId>
         <Speed>180</Speed>
         <MinDamage>60</MinDamage>
         <MaxDamage>100</MaxDamage>
         <LifetimeMS>475</LifetimeMS>
         <Amplitude>0.5</Amplitude>
         <Frequency>2</Frequency>
      </Projectile>
      <BagType>4</BagType>
      <FameBonus>5</FameBonus>
      <NumProjectiles>2</NumProjectiles>
      <ArcGap>0</ArcGap>
      <OldSound>magicShoot</OldSound>
      <feedPower>600</feedPower>
   </Object>
